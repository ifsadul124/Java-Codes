class StudentInfo {
    String name, id, email, phone, address;
    int age;
    void displayInfo() {
        System.out.println("Name: " + name);
        System.out.println("ID: " + id);
        System.out.println("Age: " + age);
        System.out.println("Email: " + email);
        System.out.println("Phone: " + phone);
        System.out.println("Address: " + address);
    }
 }
 class Grandfather {
    void company() {
        System.out.println("Company: OpenAI Technologies");
    }
 }
 public class Main {
    public static void main(String[] args) {
    StudentInfo s = new StudentInfo();
    s.name = "Siam";
    s.id = "CCE E251039";
    s.age = 21;
    s.email = "ifsadul124@gmail.com";
    s.phone = "01304620434";
    s.address = "Andorkillah, Chittagong";
    s.displayInfo();
    Grandfather g = new Grandfather();
    g.company();
    }
}
